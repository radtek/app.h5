{
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"requirePragma": false,
	"insertPragma": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "avoid",
	"rangeStart": 0,
	"proseWrap": "preserve",
	"parser": "babylon"
}
